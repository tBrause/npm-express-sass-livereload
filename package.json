{
  "name": "express-pug-sass-starter",
  "version": "0.0.3",
  "keywords": [
    "express",
    "pug",
    "sass",
    "dev",
    "node",
    "nodemon",
    "concurrently",
    "livereload"
  ],
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "concurrently \"nodemon --ext *\" \"sass --watch src/scss/main.scss:public/stylesheets/style.css --style compressed\""
  },
  "dependencies": {
    "cookie-parser": "~1.4.7",
    "debug": "~4.3.7",
    "express": "^4.21.1",
    "http-errors": "~2.0.0",
    "morgan": "~1.10.0",
    "pug": "^3.0.3"
  },
  "devDependencies": {
    "concurrently": "^9.0.1",
    "nodemon": "^3.1.7",
    "sass": "^1.80.4",
    "connect-livereload": "^0.6.1",
    "livereload": "^0.9.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tBrause/npm-express-sass-livereload.git"
  }
}